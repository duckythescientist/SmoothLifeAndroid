<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="SeekBarPreference">
        <!--https://kmansoft.com/2011/01/02/implementing-a-styleable-preference-for-a-list-of-integers/-->
        <attr name="smooth" format="boolean" />
        <attr name="min" format="integer" />
        <attr name="max" format="integer" />
        <attr name="seekBarIncrement" format="integer" />
        <attr name="adjustable" format="boolean" />
        <attr name="android_max" format="integer" />
        <attr name="showSeekBarValue" format="boolean" />
    </declare-styleable>

</resources>